
CREATE TABLE AGENCIA(
	NUMERO INT(11) PRIMARY KEY,
	GERENTE VARCHAR(30),
	LOCALIZACAO ENUM ('MG','SP','RJ'),
	TELEFONE VARCHAR(30) 
);

CREATE TABLE CLIENTE(
	NOME VARCHAR(30),
	CPF VARCHAR(30) PRIMARY KEY,
	CONTATO VARCHAR(30)
);

CREATE TABLE CONTA(
	IDCONTA INT PRIMARY KEY AUTO_INCREMENT,
	TITULAR VARCHAR(30) ,
	NUMERO INT(11) UNIQUE,
	TIPO_CONTA ENUM('CC','CP'),
	SALDO INT(11),
	NUMERO_AGENCIA INT(11),
	

	FOREIGN KEY(NUMERO_AGENCIA)
	REFERENCES AGENCIA(NUMERO)

);

ALTER TABLE CONTA 
ADD CONSTRAINT TITULAR
FOREIGN KEY (TITULAR) REFERENCES CLIENTE(CPF);

